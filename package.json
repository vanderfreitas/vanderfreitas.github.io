{
  "name": "vanderfreitas.github.io",
  "version": "3.0.1",
  "homepage": "https://vanderfreitas.github.io/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vanderfreitas/vanderfreitas.github.io.git"
  },
  "scripts": {
    "build": "rimraf ./build && react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "lint": "eslint src",
    "test": "./node_modules/.bin/react-scripts test --watchAll=false",
    "analyze": "npm run build && source-map-explorer build/static/js/*.chunk.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "gh-pages": "^6.2.0",
    "loader-utils": "^3.3.1",
    "minimatch": "^10.0.1",
    "node-fetch": "^3.3.2",
    "nth-check": "^2.1.1",
    "postcss": "^8.4.47",
    "prop-types": "^15.8.1",
    "qs": "^6.13.0",
    "react": "^18.3.1",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^18.3.1",
    "react-ga": "^3.3.1",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.27.0",
    "react-scripts": "^5.0.1",
    "react-snap": "^1.23.0",
    "react-twitter-embed": "^4.0.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  },
  "overrides": {
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
    "raw.macro": "^0.6.2",
    "sass": "^1.89.2",
    "source-map-explorer": "^2.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!react-markdown|unist-util-visit-parents|unist-util-visit|mdast-util-to-hast|remark-rehype)/"
    ]
  }
}
